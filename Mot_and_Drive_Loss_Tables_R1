%Inverter Semiconductor Drive Loss Calculations

switch Inverter_Type
   
    case 'Full_SiC_1200V'
        x= (Motor_phase_current_table)/2;
        Drive_semiconductor_loss_data =0.0323.*(x).^2 - 0.1680.*(x) + 16.9552;

    case 'Hyb_SiC_1200V'
        x= (Motor_phase_current_table)/2;
        Drive_semiconductor_loss_data =0.0071.*(x).^2 + 5.0861.*(x) + 3.7467;
               
    case 'IGBT_1200V'
        x=(Motor_phase_current_table)/2;
        Drive_semiconductor_loss_data = 0.0060*(x).^2 + 8.6460.*(x) + 20.0266;
        
        case 'IGBT_600V'
        x=(Motor_phase_current_table)/2;
        Drive_semiconductor_loss_data = 0.0175*(x).^2 + 10.8240.*(x) +24.6761;
end
